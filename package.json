{
  "name": "atomist-skill",
  "version": "0.1.0",
  "scripts": {
    "build:watch": "shadow-cljs watch dev -A:dev:test",
    "build": "shadow-cljs release release -A:dev:build",
    "bundle": "bundle",
    "skill": "npm run bundle",
    "atm:lint:prettier": "prettier --write"
  },
  "dependencies": {
    "@atomist/api-cljs": "^0.5.65",
    "js-yaml": "^3.14.1"
  },
  "devDependencies": {
    "@atomist/skill-bundler": "^0.1.28",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.0",
    "shadow-cljs": "^2.13.0",
    "source-map-support": "0.5.9",
    "ws": "^7.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.@(graphql|json|markdown|yaml|yml|md)": "npm run atm:lint:prettier"
  }
}
